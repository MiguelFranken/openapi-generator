import { Type } from "class-transformer";

{{#vars}}
  {{#isDate}}
    export function Type{{classname}}{{{name}}}() {
      return Date;
    }
  {{/isDate}}
  {{#isDateTime}}
    export function Type{{classname}}{{{name}}}() {
      return Date;
    }
  {{/isDateTime}}
  {{^isPrimitiveType}}
    {{#isModel}}
      export function Type{{classname}}{{{name}}}() {
        return {{{dataType}}};
      }
    {{/isModel}}
    {{^isModel}}
      export function Type{{classname}}{{{name}}}() {
        return {{{classVarName}}};
      }
    {{/isModel}}
  {{/isPrimitiveType}}
{{/vars}}

// @dynamic
export class {{classname}} { {{>modelGenericAdditionalProperties}}
{{#vars}}
    {{#description}}
    /**
     * {{{description}}}
     */
    {{/description}}
    {{#isDate}}@Type(Type{{classname}}{{{name}}}){{/isDate}}
    {{#isDateTime}}@Type(Type{{classname}}{{{name}}}){{/isDateTime}}
    {{^isPrimitiveType}}@Type(Type{{classname}}{{{name}}}){{/isPrimitiveType}}
    {{#isReadOnly}}readonly {{/isReadOnly}}{{{name}}}{{^required}}?{{/required}}: {{#isEnum}}{{{datatypeWithEnum}}}{{/isEnum}}{{^isEnum}}{{{dataType}}}{{/isEnum}}{{#isNullable}} | null{{/isNullable}};
{{/vars}}
}{{>modelGenericEnums}}
